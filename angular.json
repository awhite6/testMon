{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "test-lib": {
      "projectType": "library",
      "root": "projects/test-lib",
      "sourceRoot": "projects/test-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/test-lib/tsconfig.lib.json",
            "project": "projects/test-lib/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/test-lib/src/test.ts",
            "tsConfig": "projects/test-lib/tsconfig.spec.json",
            "karmaConfig": "projects/test-lib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/test-lib/tsconfig.lib.json",
              "projects/test-lib/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "form-renderer": {
      "projectType": "library",
      "root": "projects/form-renderer",
      "sourceRoot": "projects/form-renderer/src",
      "prefix": "form-renderer",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/form-renderer/tsconfig.lib.json",
            "project": "projects/form-renderer/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/form-renderer/src/test.ts",
            "tsConfig": "projects/form-renderer/tsconfig.spec.json",
            "karmaConfig": "projects/form-renderer/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/form-renderer/tsconfig.lib.json",
              "projects/form-renderer/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "defaultProject": "test-lib"
}